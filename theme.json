{
    "$schema": "https://schemas.wp.org/wp/6.4/theme.json",
    "version": 2,
    "title": "Professional Project Theme",
    "settings": {
        "appearanceTools": true,
        "color": {
            "defaultGradients": true,
            "defaultPalette": true,
            "duotone": [
                {
                    "colors": ["#007cba", "#333333"],
                    "name": "Primary and dark gray",
                    "slug": "primary-and-dark-gray"
                }
            ],
            "palette": [
                {
                    "color": "#ffffff",
                    "name": "Base",
                    "slug": "base"
                },
                {
                    "color": "#333333",
                    "name": "Contrast",
                    "slug": "contrast"
                },
                {
                    "color": "#007cba",
                    "name": "Primary",
                    "slug": "primary"
                },
                {
                    "color": "#005a87",
                    "name": "Secondary",
                    "slug": "secondary"
                },
                {
                    "color": "#00d084",
                    "name": "Accent",
                    "slug": "accent"
                },
                {
                    "color": "#f1f1f1",
                    "name": "Tertiary",
                    "slug": "tertiary"
                }
            ]
        },
        "custom": {
            "lineHeight": {
                "small": 1.2,
                "medium": 1.4,
                "large": 1.6,
                "xLarge": 1.8
            },
            "spacing": {
                "small": "0.5rem",
                "medium": "1rem",
                "large": "2rem",
                "xLarge": "3rem"
            }
        },
        "layout": {
            "contentSize": "650px",
            "wideSize": "1200px"
        },
        "spacing": {
            "blockGap": true,
            "defaultSpacingSizes": true,
            "margin": true,
            "padding": true,
            "units": ["%", "px", "em", "rem", "vh", "vw"]
        },
        "typography": {
            "defaultFontSizes": true,
            "dropCap": true,
            "fontFamilies": [
                {
                    "fontFamily": "-apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen-Sans, Ubuntu, Cantarell, 'Helvetica Neue', sans-serif",
                    "name": "System Font",
                    "slug": "system-font"
                },
                {
                    "fontFamily": "Georgia, 'Times New Roman', serif",
                    "name": "Serif",
                    "slug": "serif"
                },
                {
                    "fontFamily": "'Courier New', Courier, monospace",
                    "name": "Monospace",
                    "slug": "monospace"
                }
            ],
            "fontSizes": [
                {
                    "name": "Small",
                    "size": "0.875rem",
                    "slug": "small"
                },
                {
                    "name": "Base",
                    "size": "1rem",
                    "slug": "base"
                },
                {
                    "name": "Large",
                    "size": "1.25rem",
                    "slug": "large"
                },
                {
                    "name": "X-Large",
                    "size": "1.5rem",
                    "slug": "x-large"
                },
                {
                    "name": "XX-Large",
                    "size": "2rem",
                    "slug": "xx-large"
                },
                {
                    "name": "XXX-Large",
                    "size": "2.5rem",
                    "slug": "xxx-large"
                }
            ]
        }
    },
    "styles": {
        "blocks": {
            "core/button": {
                "border": {
                    "radius": "4px"
                },
                "color": {
                    "background": "var(--wp--preset--color--primary)",
                    "text": "var(--wp--preset--color--base)"
                },
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--small)",
                    "fontWeight": "500"
                }
            },
            "core/post-title": {
                "typography": {
                    "fontFamily": "var(--wp--preset--font-family--system-font)",
                    "fontWeight": "600"
                }
            },
            "core/post-content": {
                "spacing": {
                    "margin": {
                        "top": "var(--wp--custom--spacing--large)",
                        "bottom": "var(--wp--custom--spacing--large)"
                    }
                }
            },
            "core/site-title": {
                "typography": {
                    "fontFamily": "var(--wp--preset--font-family--system-font)",
                    "fontSize": "var(--wp--preset--font-size--large)",
                    "fontWeight": "600"
                },
                "elements": {
                    "link": {
                        "color": {
                            "text": "var(--wp--preset--color--contrast)"
                        }
                    }
                }
            },
            "core/site-tagline": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--small)",
                    "opacity": "0.7"
                }
            }
        },
        "color": {
            "background": "var(--wp--preset--color--base)",
            "text": "var(--wp--preset--color--contrast)"
        },
        "elements": {
            "h1": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--xxx-large)",
                    "lineHeight": "var(--wp--custom--line-height--small)"
                }
            },
            "h2": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--xx-large)",
                    "lineHeight": "var(--wp--custom--line-height--small)"
                }
            },
            "h3": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--x-large)",
                    "lineHeight": "var(--wp--custom--line-height--small)"
                }
            },
            "h4": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--large)",
                    "lineHeight": "var(--wp--custom--line-height--medium)"
                }
            },
            "h5": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--base)",
                    "lineHeight": "var(--wp--custom--line-height--medium)"
                }
            },
            "h6": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--small)",
                    "lineHeight": "var(--wp--custom--line-height--medium)"
                }
            },
            "link": {
                "color": {
                    "text": "var(--wp--preset--color--primary)"
                }
            }
        },
        "spacing": {
            "blockGap": "var(--wp--custom--spacing--medium)",
            "padding": {
                "top": "var(--wp--custom--spacing--medium)",
                "right": "var(--wp--custom--spacing--medium)",
                "bottom": "var(--wp--custom--spacing--medium)",
                "left": "var(--wp--custom--spacing--medium)"
            }
        },
        "typography": {
            "fontFamily": "var(--wp--preset--font-family--system-font)",
            "fontSize": "var(--wp--preset--font-size--base)",
            "lineHeight": "var(--wp--custom--line-height--large)"
        }
    },
    "templateParts": [
        {
            "name": "header",
            "title": "Header",
            "area": "header"
        },
        {
            "name": "footer",
            "title": "Footer",
            "area": "footer"
        },
        {
            "name": "sidebar",
            "title": "Sidebar",
            "area": "uncategorized"
        }
    ]
}